<div class="container-fluid container-home">
  <div class="col-md-2"></div>
  <div class="col-md-8">
    <h1 class="home">Welcome <%= current_user.nickname %></h1>
    <%= image_tag current_user.image, class: "img-circle img-thumbnail home-image" %>
    <h2 class="home"> <%= current_user.name %></h2>
    <h2 class="home"> <%= current_user.email %> </h2>
    <h2 class="home"> <%= current_user.bio %> </h2>
    <h2 class="home"> <%= current_user.website %> </h2>
    <%= link_to "View Your Photos", user_pictures_path, class: "btn btn-lg btn-default btn-home" %>
    <%= link_to "View Your Friends", "/logout", class: "btn btn-lg btn-default btn-home" %>
    <%= link_to "View the Dogs of Instagram", "/logout", class: "btn btn-lg btn-default btn-home" %>
    <%= link_to "Logout", "/logout", class: "btn btn-lg btn-default btn-home" %>
  </div>
  <div class="col-md-2"></div>
</div>
